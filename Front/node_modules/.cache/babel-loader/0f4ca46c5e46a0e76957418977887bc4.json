{"ast":null,"code":"import GenNav from \"@/components/PokeDex/gens/GenNav.vue\";\nimport axios from \"axios\";\nexport default {\n  components: {\n    GenNav\n  },\n\n  data() {\n    return {\n      pokemons: [],\n      fadeIn: null,\n      modalShow: \"modal modal_close_modal\"\n    };\n  },\n\n  created() {\n    this.getPokemons(1, 151);\n  },\n\n  methods: {\n    hideModal() {\n      this.modalShow = \"modal modal_close_modal\";\n    },\n\n    showModal() {\n      this.modalShow = \"modal modal_show\";\n    },\n\n    sendInfo(pokeInfo) {\n      this.showModal();\n      console.log(pokeInfo);\n    },\n\n    async getPokemons(start, amount) {\n      let i = start;\n      let pokemons = new Array(amount);\n\n      for await (const x of pokemons) {\n        const response = await axios.get(`https://pokeapi.co/api/v2/pokemon/${i}`);\n        let pokemon = {\n          sprite: response.data.sprites.front_default,\n          sprite2: response.data.sprites.front_default,\n          shiny: response.data.sprites.front_shiny,\n          name: response.data.name,\n          id: response.data.id,\n          types: response.data.types\n        }; // console.log(pokemon.types);\n\n        this.pokemons.push(pokemon);\n        i++;\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAkCA,OAAOA,MAAP,MAAmB,sCAAnB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,eAAe;EACbC,UAAU,EAAE;IACVF;EADU,CADC;;EAIbG,IAAI,GAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EADL;MAELC,MAAM,EAAE,IAFH;MAGLC,SAAS,EAAE;IAHN,CAAP;EAKD,CAVY;;EAWbC,OAAO,GAAG;IACR,KAAKC,WAAL,CAAiB,CAAjB,EAAoB,GAApB;EACD,CAbY;;EAcbC,OAAO,EAAE;IACPC,SAAS,GAAG;MACV,KAAKJ,SAAL,GAAiB,yBAAjB;IACD,CAHM;;IAIPK,SAAS,GAAG;MACV,KAAKL,SAAL,GAAiB,kBAAjB;IACD,CANM;;IAOPM,QAAQ,CAACC,QAAD,EAAW;MACjB,KAAKF,SAAL;MACAG,OAAO,CAACC,GAAR,CAAYF,QAAZ;IACD,CAVM;;IAWP,MAAML,WAAN,CAAkBQ,KAAlB,EAAyBC,MAAzB,EAAiC;MAC/B,IAAIC,IAAIF,KAAR;MACA,IAAIZ,QAAO,GAAI,IAAIe,KAAJ,CAAUF,MAAV,CAAf;;MACA,WAAW,MAAMG,CAAjB,IAAsBhB,QAAtB,EAAgC;QAC9B,MAAMiB,QAAO,GAAI,MAAMpB,KAAK,CAACqB,GAAN,CACpB,qCAAoCJ,CAAE,EADlB,CAAvB;QAGA,IAAIK,OAAM,GAAI;UACZC,MAAM,EAAEH,QAAQ,CAAClB,IAAT,CAAcsB,OAAd,CAAsBC,aADlB;UAEZC,OAAO,EAAEN,QAAQ,CAAClB,IAAT,CAAcsB,OAAd,CAAsBC,aAFnB;UAGZE,KAAK,EAAEP,QAAQ,CAAClB,IAAT,CAAcsB,OAAd,CAAsBI,WAHjB;UAIZC,IAAI,EAAET,QAAQ,CAAClB,IAAT,CAAc2B,IAJR;UAKZC,EAAE,EAAEV,QAAQ,CAAClB,IAAT,CAAc4B,EALN;UAMZC,KAAK,EAAEX,QAAQ,CAAClB,IAAT,CAAc6B;QANT,CAAd,CAJ8B,CAY9B;;QACA,KAAK5B,QAAL,CAAc6B,IAAd,CAAmBV,OAAnB;QACAL,CAAC;MACH;IACD;;EA9BM;AAdI,CAAf","names":["GenNav","axios","components","data","pokemons","fadeIn","modalShow","created","getPokemons","methods","hideModal","showModal","sendInfo","pokeInfo","console","log","start","amount","i","Array","x","response","get","pokemon","sprite","sprites","front_default","sprite2","shiny","front_shiny","name","id","types","push"],"sourceRoot":"","sources":["D:\\Programaci√≥n\\HieN\\poke-web\\src\\views\\PokeDex.vue"],"sourcesContent":["<template>\r\n  <GenNav />\r\n  <div class=\"container fade-in\">\r\n    <div v-for=\"(data, index) in pokemons\" :key=\"index\" class=\"poke-card\">\r\n      <h5>\r\n        {{ data.name }} <small>#{{ data.id }}</small>\r\n      </h5>\r\n      <img\r\n        class=\"sprite\"\r\n        :src=\"data.sprite\"\r\n        @mouseover=\"data.sprite = data.shiny\"\r\n        @mouseout=\"data.sprite = data.sprite2\"\r\n        @click=\"sendInfo(data)\"\r\n        :alt=\"data.name\"\r\n      />\r\n      <h6 v-for=\"(type, index) in data.types\" :key=\"index\">\r\n        {{ type.name }}\r\n      </h6>\r\n    </div>\r\n\r\n    <section :class=\"modalShow\">\r\n      <div class=\"modal_container\">\r\n        <h2 class=\"modal_title\"></h2>\r\n        <p class=\"modal_p\">\r\n          Lorem ipsum dolor, sit amet consectetur adipisicing elit. Soluta error\r\n          labore ducimus ut, illum fugit, enim eveniet quidem iusto expedita,\r\n          consectetur ipsum? Nulla, corporis a.\r\n        </p>\r\n        <button class=\"modal_close\" @click=\"hideModal\">Cerrar</button>\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n<script>\r\nimport GenNav from \"@/components/PokeDex/gens/GenNav.vue\";\r\nimport axios from \"axios\";\r\nexport default {\r\n  components: {\r\n    GenNav,\r\n  },\r\n  data() {\r\n    return {\r\n      pokemons: [],\r\n      fadeIn: null,\r\n      modalShow: \"modal modal_close_modal\",\r\n    };\r\n  },\r\n  created() {\r\n    this.getPokemons(1, 151);\r\n  },\r\n  methods: {\r\n    hideModal() {\r\n      this.modalShow = \"modal modal_close_modal\";\r\n    },\r\n    showModal() {\r\n      this.modalShow = \"modal modal_show\";\r\n    },\r\n    sendInfo(pokeInfo) {\r\n      this.showModal();\r\n      console.log(pokeInfo);\r\n    },\r\n    async getPokemons(start, amount) {\r\n      let i = start;\r\n      let pokemons = new Array(amount);\r\n      for await (const x of pokemons) {\r\n        const response = await axios.get(\r\n          `https://pokeapi.co/api/v2/pokemon/${i}`\r\n        );\r\n        let pokemon = {\r\n          sprite: response.data.sprites.front_default,\r\n          sprite2: response.data.sprites.front_default,\r\n          shiny: response.data.sprites.front_shiny,\r\n          name: response.data.name,\r\n          id: response.data.id,\r\n          types: response.data.types,\r\n        };\r\n        // console.log(pokemon.types);\r\n        this.pokemons.push(pokemon);\r\n        i++;\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\n.poke-card {\r\n  background-color: aliceblue;\r\n\r\n  border-radius: 20px;\r\n  width: 10rem;\r\n\r\n  -webkit-box-shadow: 5px 5px 15px -3px rgba(0, 0, 0, 0.51);\r\n  box-shadow: 5px 5px 15px -3px rgba(0, 0, 0, 0.51);\r\n}\r\n.poke-card h5 {\r\n  position: relative;\r\n  bottom: 22px;\r\n  background-color: rgb(207, 232, 255);\r\n\r\n  border-top-right-radius: 20px;\r\n  border-top-left-radius: 20px;\r\n  border-bottom-left-radius: none;\r\n  border-bottom-right-radius: none;\r\n}\r\n\r\n.sprite {\r\n  cursor: pointer;\r\n}\r\n\r\n.container {\r\n  display: grid;\r\n  grid-template-columns: repeat(auto-fit, minmax(10rem, 1fr));\r\n  gap: 1em;\r\n}\r\nh5::first-letter {\r\n  text-transform: uppercase;\r\n}\r\n.modal {\r\n  position: fixed;\r\n  top: 0;\r\n  bottom: 0;\r\n  left: 0;\r\n  right: 0;\r\n  background-color: #111111bd;\r\n  display: flex;\r\n}\r\n.modal_container {\r\n  margin: auto;\r\n  width: 90%;\r\n  max-width: 600px;\r\n  max-height: 90%;\r\n  background-color: white;\r\n  border-radius: 20px;\r\n  padding: 3em 2.5em;\r\n  display: grid;\r\n  grid-auto-columns: 100%;\r\n}\r\n\r\n.modal_close {\r\n  background-color: #b94242;\r\n  color: white;\r\n  transition: 0.2s;\r\n  border: 1px solid;\r\n  border-radius: 6px;\r\n  transition: 0.3s;\r\n  cursor: pointer;\r\n}\r\n.modal_close:hover {\r\n  transition: 0.2s;\r\n  background-color: white;\r\n  color: #b94242;\r\n}\r\n.modal_show {\r\n  opacity: 1;\r\n  pointer-events: unset;\r\n  transition: 0.3s;\r\n}\r\n.modal_close_modal {\r\n  opacity: 0;\r\n  pointer-events: none;\r\n  transition: 0.3s;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}