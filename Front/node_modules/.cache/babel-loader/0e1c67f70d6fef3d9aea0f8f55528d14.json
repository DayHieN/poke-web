{"ast":null,"code":"import getPokemonOptions from \"@/helpers/vuekemonUtilidades.js\";\nimport VuekemonImg from \"@/components/pokeGame/VuekemonImg.vue\";\nimport VuekemonOpciones from \"@/components/pokeGame/VuekemonOpciones.vue\";\nexport default {\n  data() {\n    return {\n      pokemon: null,\n      hayRespuesta: false,\n      listaArray: null\n    };\n  },\n\n  components: {\n    VuekemonImg,\n    VuekemonOpciones\n  },\n  methods: {\n    activarRespuesta(data) {\n      this.hayRespuesta = true;\n\n      if (data === this.pokemon.id) {\n        this.msg = `¡Correcto! ¡Es ${this.pokemon.name}!`;\n      } else {\n        this.msg = `¡Qué pena! ¡Es ${this.pokemon.name}!`;\n      }\n    },\n\n    async cargar() {\n      this.listaArray = await getPokemonOptions();\n      const rndInt = Math.floor(Math.random() * 4);\n      this.pokemon = this.listaArray[rndInt];\n    }\n\n  },\n\n  mounted() {\n    this.cargar();\n  }\n\n};","map":{"version":3,"mappings":"AAkBA,OAAOA,iBAAP,MAA8B,iCAA9B;AACA,OAAOC,WAAP,MAAwB,uCAAxB;AACA,OAAOC,gBAAP,MAA6B,4CAA7B;AAEA,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,OAAO,EAAE,IADJ;MAELC,YAAY,EAAE,KAFT;MAGLC,UAAU,EAAE;IAHP,CAAP;EAKD,CAPY;;EAQbC,UAAU,EAAE;IACVN,WADU;IAEVC;EAFU,CARC;EAYbM,OAAO,EAAE;IACPC,gBAAgB,CAACN,IAAD,EAAO;MACrB,KAAKE,YAAL,GAAoB,IAApB;;MACA,IAAIF,IAAG,KAAM,KAAKC,OAAL,CAAaM,EAA1B,EAA8B;QAC5B,KAAKC,GAAL,GAAY,kBAAiB,KAAKP,OAAL,CAAaQ,IAAK,GAA/C;MACF,CAFA,MAEO;QACL,KAAKD,GAAL,GAAY,kBAAiB,KAAKP,OAAL,CAAaQ,IAAK,GAA/C;MACF;IACD,CARM;;IASP,MAAMC,MAAN,GAAe;MACb,KAAKP,UAAL,GAAkB,MAAMN,iBAAiB,EAAzC;MACA,MAAMc,MAAK,GAAIC,IAAI,CAACC,KAAL,CAAWD,IAAI,CAACE,MAAL,KAAgB,CAA3B,CAAf;MACA,KAAKb,OAAL,GAAe,KAAKE,UAAL,CAAgBQ,MAAhB,CAAf;IACD;;EAbM,CAZI;;EA2BbI,OAAO,GAAG;IACR,KAAKL,MAAL;EACD;;AA7BY,CAAf","names":["getPokemonOptions","VuekemonImg","VuekemonOpciones","data","pokemon","hayRespuesta","listaArray","components","methods","activarRespuesta","id","msg","name","cargar","rndInt","Math","floor","random","mounted"],"sourceRoot":"","sources":["D:\\Pr