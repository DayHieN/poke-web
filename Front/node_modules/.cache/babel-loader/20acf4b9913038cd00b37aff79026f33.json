{"ast":null,"code":"import getPokemonOptions from \"@/helpers/vuekemonUtilidades.js\";\nimport PokeGameImg from \"@/components/pokeGame/PokeGameImg.vue\";\nimport PokeGameOptions from \"@/components/pokeGame/PokeGameOptions.vue\";\nexport default {\n  data() {\n    return {\n      pokemon: null,\n      hayRespuesta: false,\n      listaArray: null,\n      msg: null,\n      mostrarLista: true\n    };\n  },\n\n  components: {\n    PokeGameImg,\n    PokeGameOptions\n  },\n  methods: {\n    activarRespuesta(data) {\n      this.hayRespuesta = true;\n      this.mostrarLista = false;\n\n      if (data === this.pokemon.id) {\n        this.msg = `¡Correcto! ¡Es ${this.pokemon.name}!`;\n      } else {\n        this.msg = `¡Qué pena! ¡Es ${this.pokemon.name}!`;\n      }\n    },\n\n    async cargar() {\n      this.listaArray = await getPokemonOptions();\n      const rndInt = Math.floor(Math.random() * 4);\n      this.pokemon = this.listaArray[rndInt];\n    },\n\n    newGame() {\n      this.pokemon = null, this.hayRespuesta = false;\n      this.listaArray = null;\n      this.msg = null;\n      this.mostrarLista = true;\n      this.cargar();\n    }\n\n  },\n\n  mounted() {\n    this.cargar();\n  }\n\n};","map":{"version":3,"mappings":"AAoBA,OAAOA,iBAAP,MAA8B,iCAA9B;AACA,OAAOC,WAAP,MAAwB,uCAAxB;AACA,OAAOC,eAAP,MAA4B,2CAA5B;AAEA,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,OAAO,EAAE,IADJ;MAELC,YAAY,EAAE,KAFT;MAGLC,UAAU,EAAE,IAHP;MAILC,GAAG,EAAC,IAJC;MAKLC,YAAY,EAAC;IALR,CAAP;EAOD,CATY;;EAUbC,UAAU,EAAE;IACVR,WADU;IAEVC;EAFU,CAVC;EAcbQ,OAAO,EAAE;IACPC,gBAAgB,CAACR,IAAD,EAAO;MACrB,KAAKE,YAAL,GAAoB,IAApB;MACA,KAAKG,YAAL,GAAoB,KAApB;;MACA,IAAIL,IAAG,KAAM,KAAKC,OAAL,CAAaQ,EAA1B,EAA8B;QAC5B,KAAKL,GAAL,GAAY,kBAAiB,KAAKH,OAAL,CAAaS,IAAK,GAA/C;MACF,CAFA,MAEO;QACL,KAAKN,GAAL,GAAY,kBAAiB,KAAKH,OAAL,CAAaS,IAAK,GAA/C;MACF;IACD,CATM;;IAUP,MAAMC,MAAN,GAAe;MACb,KAAKR,UAAL,GAAkB,MAAMN,iBAAiB,EAAzC;MACA,MAAMe,MAAK,GAAIC,IAAI,CAACC,KAAL,CAAWD,IAAI,CAACE,MAAL,KAAgB,CAA3B,CAAf;MACA,KAAKd,OAAL,GAAe,KAAKE,UAAL,CAA